<div class="note twelve columns panel">
  <div class="two columns">
    <%= note.created_at.strftime("%d %b %Y") %>
  </div>
  <div class="ten columns">
    <div class="twelve columns">
      <h4><%= note.subject %></h4>
    </div>
    <div class="twelve columns">
      <%= note.content %>
    </div>
  </div>
</div>